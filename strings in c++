#include <iostream>
#include <string>
using namespace std;

int main() {
	// Complete the program
    string a,b;
    cin>>a;
    cin>>b;
    cout<<a.size()<<" ";
    cout<<b.size()<<endl;
    cout<<a+b<<endl;
    char letter=a[0];
    a[0]=b[0];
    b[0]=letter;
    cout<<a<<" ";
    cout<<b<<endl;
    return 0;
}
